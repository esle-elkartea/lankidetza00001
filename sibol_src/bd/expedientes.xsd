<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:element name="EXPEDIENTES">		<xs:complexType>			<xs:sequence>				<xs:element name="EXPEDIENTE">					<xs:complexType>						<xs:sequence>							<xs:element ref="CERTIFICADO"/>							<xs:element ref="MEMORIA"/>							<xs:element ref="PROVEEDOR"/>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="CERTIFICADO">		<xs:complexType>			<xs:sequence>				<xs:element ref="TITULAR"/>				<xs:element ref="REPRESENTANTE" minOccurs="0"/>				<xs:element ref="SUMINISTRADORA"/>				<xs:element ref="CUPS"/>				<xs:element ref="EMP_INSTALADORA"/>				<xs:element ref="INSTALADOR"/>				<xs:element ref="INSTALACION"/>				<xs:element ref="TIPO_INST"/>				<xs:element name="SUPERFICIE">					<xs:annotation>						<xs:documentation>Superficie de la instalacion</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element ref="TENSION"/>				<xs:element ref="POTENCIA"/>				<xs:element ref="SUMINISTRO" minOccurs="0"/>				<xs:element name="SUMINISTRO_VIV" minOccurs="0">					<xs:complexType>						<xs:sequence>							<xs:group ref="SUM" maxOccurs="unbounded"/>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="RESISTENCIA">					<xs:annotation>						<xs:documentation>Resistencia de la instalacion</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:decimal">							<xs:fractionDigits value="2"/>							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="REGLAMENTO">					<xs:annotation>						<xs:documentation>Resistencia de la instalacion</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="RBT 1973|RBT 2002"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:group name="SUM">		<xs:sequence>			<xs:element name="DESCRIPCION">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="POTENCIA">				<xs:simpleType>					<xs:restriction base="xs:decimal">						<xs:whiteSpace value="collapse"/>						<xs:fractionDigits value="2"/>						<xs:totalDigits value="5"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="TIPO">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:length value="1"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element ref="TENSION"/>			<xs:element name="TIP_USO">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:pattern value="VI|SG|GA|AS|UF|EA|UV|AT|TR|ES|TL"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:group>	<xs:element name="MEMORIA">		<xs:complexType>			<xs:sequence>				<xs:element ref="TITULAR"/>				<xs:element ref="TENSION"/>				<xs:element ref="POTENCIA"/>				<xs:element name="MEMORIA_POR">					<xs:complexType>						<xs:sequence>							<xs:element name="MOTIVO">								<xs:annotation>									<xs:documentation>Motivo de la memoria (Nueva instalacion, Reforma, Cambio de Nombre, Cambio de Tension, Otra Descripcion.)</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:pattern value="NU|AR|CN|CT|OD"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="OTRA_DESC" minOccurs="0">								<xs:annotation>									<xs:documentation>Descripcion en caso de ser diferente a los especificados</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="30"/>									</xs:restriction>								</xs:simpleType>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element ref="TIPO_INST"/>				<xs:element name="SUPERFICIE">					<xs:annotation>						<xs:documentation>Superficie de la instalacion</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element ref="DATOS_TECNICOS"/>				<xs:element ref="CARGAS_INDUSTRIALES" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="CARGAS_VIVIENDAS" minOccurs="0"/>				<xs:element ref="CARGAS_OFICINAS" minOccurs="0"/>				<xs:element ref="INSTALADOR"/>				<xs:element ref="CUADRO_CIRCUITOS"/>				<xs:element name="MEMORIA_DESCRIPTIVA" type="xs:string"/>				<xs:element name="ESQUEMA_UNIFILAR" type="xs:base64Binary" minOccurs="0">					<xs:annotation>						<xs:documentation>Documento en binario Base 64</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="PROVEEDOR">		<xs:annotation>			<xs:documentation>Proveedor de Software</xs:documentation>		</xs:annotation>		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:minLength value="3"/>				<xs:maxLength value="20"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="ACOMETIDA">		<xs:complexType>			<xs:sequence>				<xs:element name="TIPO">					<xs:annotation>						<xs:documentation>Tipo de Circuito</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="0"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="DATOS_CIRCUITOS"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="ALUMBRADO">		<xs:complexType>			<xs:sequence>				<xs:element name="TIPO">					<xs:annotation>						<xs:documentation>Cuadro de circuitos de alumbrado</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="DESCRIPCION" minOccurs="0">					<xs:annotation>						<xs:documentation>Descripcion del circuito</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="25"/>							<xs:pattern value="Portal|Garaje|Escaleras"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="DATOS_CIRCUITOS"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="CARGAS_INDUSTRIALES">		<xs:complexType>			<xs:sequence>				<xs:element name="TIPO">					<xs:annotation>						<xs:documentation>Tipo de carga (F=Fuerza, A=Alumbrado)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:length value="1"/>							<xs:pattern value="F|A"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="DESCRIPCION">					<xs:annotation>						<xs:documentation>Descripcion de suministro</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="30"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="POTENCIA" minOccurs="0">					<xs:annotation>						<xs:documentation>Potencia especifica prevista (W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="6"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="CARGAS_OFICINAS">		<xs:complexType>			<xs:sequence>				<xs:element name="TOTAL_OFICINAS" minOccurs="0">					<xs:annotation>						<xs:documentation>Numero total de oficinas</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="SUPERFICIE_OFICINAS" minOccurs="0">					<xs:annotation>						<xs:documentation>Superficie total de oficinas</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="4"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="POTENCIA_OF" minOccurs="0">					<xs:annotation>						<xs:documentation>Demanda maxima / oficinas(W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="ESTABLECIMIENTOS" minOccurs="0">					<xs:annotation>						<xs:documentation>Numero total de establecimientos industriales</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="SUPERFICIE_ESTABLECIMIENTOS" minOccurs="0">					<xs:annotation>						<xs:documentation>Superficie total de establecimientos industriales</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="4"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="POTENCIA_EST" minOccurs="0">					<xs:annotation>						<xs:documentation>Demanda maxima/establ. industrial (W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="ASCENSORES" minOccurs="0">					<xs:annotation>						<xs:documentation>Numero de ascensores</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="6"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="ALUMBRADO_ESCALERA" minOccurs="0">					<xs:annotation>						<xs:documentation>Alumbrado escalera</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="OTROS_SERVICIOS" minOccurs="0">					<xs:annotation>						<xs:documentation>Otros servicios</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="6"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="OTRAS_CARGAS" minOccurs="0">					<xs:annotation>						<xs:documentation>Descripcion de otras cargas</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="50"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="POTENCIA" minOccurs="0">					<xs:annotation>						<xs:documentation>Potencia prevista(W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="6"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="CARGAS_VIVIENDAS">		<xs:complexType>			<xs:sequence>				<xs:element name="ELECTRIFICACION" minOccurs="0" maxOccurs="2">					<xs:complexType>						<xs:choice>							<xs:element ref="ELECTRIFICACION_BASICO"/>							<xs:element ref="ELECTRIFICACION_ELEVADO"/>						</xs:choice>					</xs:complexType>				</xs:element>				<xs:element name="COEF_SIMULTANEIDAD">					<xs:annotation>						<xs:documentation>Coeficiente de simultaneidad</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="2"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="ASCENSORES" minOccurs="0">					<xs:annotation>						<xs:documentation>Potencia para ascensores (W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="ALUMBRADO_ESCALERA" minOccurs="0">					<xs:annotation>						<xs:documentation>Alumbrado escalera (W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="OTROS_SERVICIOS" minOccurs="0">					<xs:annotation>						<xs:documentation>Otros servicios (W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="6"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="SUPERFICIE" minOccurs="0">					<xs:annotation>						<xs:documentation>Superficie en m2</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="6"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="POTENCIA" minOccurs="0">					<xs:annotation>						<xs:documentation>Potencia especifica prevista (W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="4"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="CUADRO_CIRCUITOS">		<xs:complexType>			<xs:sequence>				<xs:element ref="ACOMETIDA" minOccurs="0"/>				<xs:element ref="LINEA_GENERAL" minOccurs="0"/>				<xs:element ref="INS_INDUSTRIALES" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="DERIVACION_INDIVIDUAL" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="VIV_TIPO" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="ALUMBRADO" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="FUERZA" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="DAT_CIRCUITOS">		<xs:complexType>			<xs:sequence>				<xs:element name="POTENCIA" minOccurs="0">					<xs:annotation>						<xs:documentation>Potencia de calculo (W)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="7"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="TEN_CAL" minOccurs="0">					<xs:annotation>						<xs:documentation>Tension de calculo (V)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="10"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="INTENSIDAD" minOccurs="0">					<xs:annotation>						<xs:documentation>Valor de intensidad (A)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:decimal">							<xs:totalDigits value="4"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="N_CONDUC" minOccurs="0">					<xs:annotation>						<xs:documentation>Numero de conductores</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="20"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="AISLAMIENTO" minOccurs="0">					<xs:annotation>						<xs:documentation>Descripcion del aislamiento (V)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="8"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element ref="TIPO_INS_CIRCTO" minOccurs="0"/>				<xs:element name="INT_MAX">					<xs:annotation>						<xs:documentation>Intensidad maxima admisible</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="4"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="CCPIA" minOccurs="0">					<xs:annotation>						<xs:documentation>CCPIA (Amperios)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="4"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="LONGITUD" minOccurs="0">					<xs:annotation>						<xs:documentation>Longitud en metros</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="4"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="CAIDA" minOccurs="0">					<xs:annotation>						<xs:documentation>Valor de tension (V)</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:decimal">							<xs:fractionDigits value="2"/>							<xs:totalDigits value="5"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="DATOS_TECNICOS">		<xs:complexType>			<xs:sequence>				<xs:element ref="PUNTO_CONEXION" minOccurs="0"/>				<xs:element ref="TIPO_ACOMETIDA" minOccurs="0"/>				<xs:element name="SECCION_ACOMETIDA" minOccurs="0">					<xs:annotation>						<xs:documentation>Descripcion de la seccion de acometida</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="15"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element ref="MATERIAL_ACOMETIDA" minOccurs="0"/>				<xs:element name="TIPO_CGP" minOccurs="0">					<xs:annotation>						<xs:documentation>Descripcion del tipo CGP</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="20"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="IN_BASE" minOccurs="0">					<xs:annotation>						<xs:documentation>Intensidad Base</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="IN_CARTUCHO" minOccurs="0">					<xs:annotation>						<xs:documentation>Intensidad del Cartucho</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:totalDigits value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element ref="TIPO_LINEA"/>				<xs:element name="SECCION_LINEA">					<xs:annotation>						<xs:documentation>Seccion de la linea</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="20"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element ref="MATERIAL_LINEA"/>				<xs:element name="TIPO_MODULO">					<xs:annotation>						<xs:documentation>Descripcion tipo de modulo</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="25"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="SITUACION_MODULO">					<xs:annotation>						<xs:documentation>Descripcion situacion de modulo</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restrict