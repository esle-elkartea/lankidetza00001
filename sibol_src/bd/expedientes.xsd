name="CODIGO">					<xs:annotation>						<xs:documentation>Codigo de la empresa suministradora</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="16"/>							<xs:minLength value="1"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="NOMBRE">					<xs:annotation>						<xs:documentation>Razon Social de la Empresa Suministradora</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="44"/>							<xs:minLength value="1"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="ESCALERA">		<xs:annotation>			<xs:documentation>Descripcion de la escalera</xs:documentation>		</xs:annotation>		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="3"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="INSTALACION">		<xs:complexType>			<xs:sequence>				<xs:element ref="PROVINCIA"/>				<xs:element ref="MUNICIPIO"/>				<xs:element ref="LOCALIDAD"/>				<xs:element ref="CALLE"/>				<xs:element ref="PORTAL"/>				<xs:element ref="CP"/>				<xs:element ref="TELEFONO"/>				<xs:element ref="EMAIL"/>				<xs:element ref="BIS"/>				<xs:element ref="ESCALERA"/>				<xs:element ref="PISO"/>				<xs:element ref="PUERTA"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="INSTALADOR">		<xs:complexType>			<xs:sequence>				<xs:element ref="NIF"/>				<xs:element name="CARNET">					<xs:annotation>						<xs:documentation>Carn?el instalador</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="16"/>							<xs:minLength value="1"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="NOMBRE">					<xs:annotation>						<xs:documentation>Nombre del instalador</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="120"/>							<xs:minLength value="1"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="CATEGORIA">					<xs:annotation>						<xs:documentation>Categoria del instalador</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:length value="1"/>							<xs:pattern value="-|B|E"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="MODALIDAD">					<xs:annotation>						<xs:documentation>Modalidad del instalador</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:length value="1"/>							<xs:pattern value="\d{1}"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="LOCALIDAD">		<xs:complexType>			<xs:sequence>				<xs:element name="CODIGO">					<xs:annotation>						<xs:documentation>Codigo de la localidad</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="3"/>							<xs:minLength value="1"/>							<xs:pattern value="\d{3}|\d{2}|\d{1}"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="NOMBRE">					<xs:annotation>						<xs:documentation>Nombre de la localidad</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="40"/>							<xs:minLength value="1"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="MUNICIPIO">		<xs:complexType>			<xs:sequence>				<xs:element name="CODIGO">					<xs:annotation>						<xs:documentation>Codigo del municipio</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="4"/>							<xs:minLength value="1"/>							<xs:pattern value="\d{4}|\d{3}|\d{2}|\d{1}"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="NOMBRE">					<xs:annotation>						<xs:documentation>Nombre del municipio</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="40"/>							<xs:minLength value="1"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="PISO">		<xs:annotation>			<xs:documentation>Descripcion del piso</xs:documentation>		</xs:annotation>		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="4"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="PORTAL">		<xs:annotation>			<xs:documentation>Descripcion del portal</xs:documentation>		</xs:annotation>		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="4"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="POTENCIA">		<xs:annotation>			<xs:documentation>Valor de potencia (W)</xs:documentation>		</xs:annotation>		<xs:simpleType>			<xs:restriction base="xs:decimal">				<xs:fractionDigits value="2"/>				<xs:totalDigits value="7"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="PROVINCIA">		<xs:complexType>			<xs:sequence>				<xs:element name="CODIGO">					<xs:annotation>						<xs:documentation>Codigo de la provincia</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="2"/>							<xs:minLength value="1"/>							<xs:pattern value="\d{2}|\d{1}"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="NOMBRE">					<xs:annotation>						<xs:documentation>Nombre de la provincia</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="25"/>							<xs:minLength value="1"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="PUERTA">		<xs:annotation>			<xs:documentation>Descripcion de la puerta</xs:documentation>		</xs:annotation>		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="3"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="REPRESENTANTE">		<xs:complexType>			<xs:sequence>				<xs:element name="NOMBRE">					<xs:annotation>						<xs:documentation>Nombre del Representante Legal de la Instalacion 						</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="50"/>							<xs:minLength value="1"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element ref="NIF"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="SUMINISTRADORA">		<xs:annotation>			<xs:documentation>Nombre de la Empresa Suministradora</xs:documentation>		</xs:annotation>		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:maxLength value="11"/>				<xs:minLength value="1"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="SUMINISTRO">		<xs:complexType>			<xs:sequence>				<xs:element name="DESCRIPCION">					<xs:annotation>						<xs:documentation>Descripcion de suministro</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:maxLength value="50"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element ref="POTENCIA"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="TELEFONO">		<xs:complexType>			<xs:sequence>				<xs:element name="PREFIJO">					<xs:annotation>					